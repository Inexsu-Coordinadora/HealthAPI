{
  "name": "healthapi",
  "version": "1.0.0",
  "description": "Sistema de gestión de citas médicas con arquitectura hexagonal",
  "main": "dist/index.js",
  "scripts": {
    "start": "node ./dist/index.js",
    "build": "rimraf ./dist && tsc",
    "dev": "tsc-watch --onSuccess \"node ./dist/index.js\"",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "lint": "eslint \"src/**/*.ts\" --fix",
    "lint:check": "eslint \"src/**/*.ts\"",
    "check": "npm run format:check && npm run lint:check && tsc --noEmit",
    "test": "jest",
    "test:unit": "jest tests/unit",
    "test:integration": "jest tests/integration",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Inexsu-Coordinadora/HealthAPI.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "module",
  "bugs": {
    "url": "https://github.com/Inexsu-Coordinadora/HealthAPI/issues"
  },
  "homepage": "https://github.com/Inexsu-Coordinadora/HealthAPI#readme",
  "dependencies": {
    "-": "^0.0.1",
    "@fastify/cors": "^11.1.0",
    "@fastify/env": "^5.0.3",
    "@fastify/swagger": "^9.6.1",
    "@fastify/swagger-ui": "^5.2.3",
    "dotenv": "^17.2.3",
    "fastify": "^5.6.2",
    "pg": "^8.16.3",
    "pino": "^10.1.0",
    "pino-pretty": "^13.1.2",
    "uuid": "^13.0.0",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@jest/globals": "^30.2.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.9.2",
    "@types/pg": "^8.15.6",
    "@types/supertest": "^6.0.3",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.2",
    "eslint": "^9.38.0",
    "jest": "^30.2.0",
    "prettier": "^3.6.2",
    "rimraf": "^6.1.0",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.5",
    "tsc-watch": "^7.2.0",
    "typescript": "^5.9.3"
  }
}